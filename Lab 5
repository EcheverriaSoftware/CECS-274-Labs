import java.util.Scanner;
import java.util.Arrays;
public class Lab_Warmup5
{
  public static void main(String [] args){
        Scanner input = new Scanner(System.in);
        String phoneNumbersCombined = "";
        int key = 0;
        while(key < 3){
            System.out.println("Enter a phone number: ");
            phoneNumbersCombined += input.nextLine();
            key++;
        }
        String [] arr = phoneNumbersCombined.split("");
        int [] integerArray = new int[arr.length];
        int [] numOfOccurnaces = new int[10];
        for(int i = 0; i < arr.length; i++){
            integerArray[i] = Integer.parseInt(arr[i]);
        }
        Arrays.sort(integerArray);
    
  }
}
